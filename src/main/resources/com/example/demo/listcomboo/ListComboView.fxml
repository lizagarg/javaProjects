<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="561.0" prefWidth="582.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.listcombo.ListComboController">
   <children>
      <ComboBox fx:id="comboItems" layoutX="217.0" layoutY="31.0" onAction="#doFill" prefWidth="150.0" />
      <ListView fx:id="listAvailableItems" layoutX="90.0" layoutY="69.0" onMouseClicked="#doAddToCart" prefHeight="181.0" prefWidth="171.0" />
      <ListView fx:id="listAvailablePrice" layoutX="321.0" layoutY="69.0" prefHeight="181.0" prefWidth="171.0" />
      <ListView fx:id="listSelectedItems" layoutX="90.0" layoutY="280.0" onMouseClicked="#doRemoveItems" prefHeight="181.0" prefWidth="171.0" />
      <ListView fx:id="listSelectedPrice" layoutX="321.0" layoutY="280.0" prefHeight="181.0" prefWidth="171.0" />
      <Button layoutX="275.0" layoutY="470.0" mnemonicParsing="false" onAction="#calBill" text="Bill" />
      <Label fx:id="billdisplay" layoutX="101.0" layoutY="507.0" text="Your Bill:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
